{
  "parameterTypes": [
    {
      "name": "isoDate",
      "regex": "\\d{4}-\\d{2}-\\d{2}"
    },
    {
      "name": "money",
      "regex": "\\$(\\d+\\.\\d{2})"
    },
    {
      "name": "size",
      "regex": "Small|Medium|Large"
    },
    {
      "name": "biginteger",
      "regex": "-?\\d+|\\d+"
    },
    {
      "name": "string",
      "regex": "\"([^\"\\\\]*(\\\\.[^\"\\\\]*)*)\"|'([^'\\\\]*(\\\\.[^'\\\\]*)*)'"
    },
    {
      "name": "bigdecimal",
      "regex": "(?=.*\\d.*)[-+]?(?:\\d+(?:[,]?\\d+)*)*(?:[.](?=\\d.*))?\\d*(?:\\d+[E]-?\\d+)?"
    },
    {
      "name": "byte",
      "regex": "-?\\d+|\\d+"
    },
    {
      "name": "double",
      "regex": "(?=.*\\d.*)[-+]?(?:\\d+(?:[,]?\\d+)*)*(?:[.](?=\\d.*))?\\d*(?:\\d+[E]-?\\d+)?"
    },
    {
      "name": "short",
      "regex": "-?\\d+|\\d+"
    },
    {
      "name": "float",
      "regex": "(?=.*\\d.*)[-+]?(?:\\d+(?:[,]?\\d+)*)*(?:[.](?=\\d.*))?\\d*(?:\\d+[E]-?\\d+)?"
    },
    {
      "name": "word",
      "regex": "[^\\s]+"
    },
    {
      "name": "int",
      "regex": "-?\\d+|\\d+"
    },
    {
      "name": "long",
      "regex": "-?\\d+|\\d+"
    }
  ],
  "steps": [
    {
      "type": "Given",
      "expression": "the coffee shop is open",
      "example": "the coffee shop is open"
    },
    {
      "type": "When",
      "expression": "I order a {string} on {isoDate}",
      "example": "I order a \"Latte\" on 2024-05-20"
    },
    {
      "type": "When",
      "expression": "I order a {size} {string} with {string}",
      "example": "I order a Large \"Latte\" with \"Milk, Sugar\""
    },
    {
      "type": "Then",
      "expression": "the receipt should show {money}",
      "example": "the receipt should show $6.50"
    },
    {
      "type": "Then",
      "expression": "I wait {int} minutes",
      "example": "I wait 5 minutes"
    },
    {
      "type": "Given",
      "expression": "the temperature is {float} degrees",
      "example": "the temperature is 98.6 degrees"
    },
    {
      "type": "Given",
      "expression": "I use the word {word}",
      "example": "I use the word Hello"
    },
    {
      "type": "Then",
      "expression": "I have {byte} coins",
      "example": "I have 127 coins"
    },
    {
      "type": "Then",
      "expression": "I count {short} sheep",
      "example": "I count 30000 sheep"
    },
    {
      "type": "Then",
      "expression": "the universe age is {long} years",
      "example": "the universe age is 13700000000 years"
    },
    {
      "type": "Then",
      "expression": "the value is {bigdecimal}",
      "example": "the value is 123.456789"
    },
    {
      "type": "Then",
      "expression": "the big integer is {biginteger}",
      "example": "the big integer is 12345678901234567890"
    },
    {
      "type": "Then",
      "expression": "the precise value is {double}",
      "example": "the precise value is 3.14159265359"
    }
  ]
}